<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.post.dao.DeptPostMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.post.model.DeptPost">
            <id column="id" property="id" />
            <result column="dept_id" property="deptId" />
            <result column="post_id" property="postId" />
            <result column="user_id" property="userId" />
            <result column="is_db" property="isDb" />
            <result column="is_star" property="isStar" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
	    id AS id,
	    dept_id AS deptId,
	    post_id AS postId,
	    user_id AS userId,
	    is_db AS isDb,
	    is_star AS isStar
    </sql>

</mapper>
