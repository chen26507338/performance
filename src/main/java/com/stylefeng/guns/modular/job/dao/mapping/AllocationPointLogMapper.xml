<mapper namespace="com.stylefeng.guns.modular.job.dao.AllocationPointLogMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.job.model.AllocationPointLog">
            <id column="id" property="id" />
