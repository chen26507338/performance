<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.assess.dao.ShpxgzAssessMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.assess.model.ShpxgzAssess">
            <id column="id" property="id" />
            <result column="user_id" property="userId" />
            <result column="assess_name" property="assessName" />
            <result column="name" property="name" />
            <result column="num" property="num" />
            <result column="main_norm_point" property="mainNormPoint" />
            <result column="year" property="year" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
	    id AS id,
	    user_id AS userId,
	    assess_name AS assessName,
	    name AS name,
	    num AS num,
	    main_norm_point AS mainNormPoint,
	    year AS year
    </sql>

</mapper>
