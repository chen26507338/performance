<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.payment.dao.JsAwardMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.payment.model.JsAward">
            <id column="id" property="id" />
            <result column="user_id" property="userId" />
            <result column="yrxs" property="yrxs" />
            <result column="project" property="project" />
            <result column="hjlb" property="hjlb" />
            <result column="hjdj" property="hjdj" />
            <result column="money" property="money" />
            <result column="year" property="year" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
	    id AS id,
	    user_id AS userId,
	    yrxs AS yrxs,
	    project AS project,
	    hjlb AS hjlb,
	    hjdj AS hjdj,
	    money AS money,
	    year AS year
    </sql>

</mapper>
