<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.user.dao.WorkResumeMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.user.model.WorkResume">
            <id column="id" property="id" />
            <result column="start_date" property="startDate" />
            <result column="end_date" property="endDate" />
            <result column="company" property="company" />
            <result column="department" property="department" />
            <result column="job" property="job" />
            <result column="title" property="title" />
            <result column="pension_month" property="pensionMonth" />
            <result column="proc_ins_id" property="procInsId" />
            <result column="status" property="status" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
	    id AS id,
	    start_date AS startDate,
	    end_date AS endDate,
	    company AS company,
	    department AS department,
	    job AS job,
	    title AS title,
	    pension_month AS pensionMonth,
	    proc_ins_id AS procInsId,
	    status AS status
    </sql>

</mapper>
