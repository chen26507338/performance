<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.user.dao.KinshipMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.user.model.Kinship">
            <id column="id" property="id" />
            <result column="relationship" property="relationship" />
            <result column="name" property="name" />
            <result column="birthday" property="birthday" />
            <result column="age" property="age" />
            <result column="politics_status" property="politicsStatus" />
            <result column="company" property="company" />
            <result column="proc_ins_id" property="procInsId" />
            <result column="status" property="status" />
            <result column="user_id" property="userId" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
	    id AS id,
	    relationship AS relationship,
	    name AS name,
	    birthday AS birthday,
	    age AS age,
	    politics_status AS politicsStatus,
	    company AS company,
	    proc_ins_id AS procInsId,
	    status AS status,
	    user_id AS userId
    </sql>

</mapper>
