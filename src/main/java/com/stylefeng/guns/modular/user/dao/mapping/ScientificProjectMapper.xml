<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.user.dao.ScientificProjectMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.user.model.ScientificProject">
            <id column="id" property="id" />
            <result column="name" property="name" />
            <result column="nature" property="nature" />
            <result column="type" property="type" />
            <result column="start_time" property="startTime" />
            <result column="end_time" property="endTime" />
            <result column="expenditure" property="expenditure" />
            <result column="rank" property="rank" />
            <result column="user_id" property="userId" />
            <result column="proc_ins_id" property="procInsId" />
            <result column="status" property="status" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
	    id AS id,
	    name AS name,
	    nature AS nature,
	    type AS type,
	    start_time AS startTime,
	    end_time AS endTime,
	    expenditure AS expenditure,
	    rank AS rank,
	    user_id AS userId,
	    proc_ins_id AS procInsId,
	    status AS status
    </sql>

</mapper>
