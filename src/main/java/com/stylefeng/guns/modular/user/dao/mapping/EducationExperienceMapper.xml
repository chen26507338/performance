<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.user.dao.EducationExperienceMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.user.model.EducationExperience">
            <id column="id" property="id" />
            <result column="enrollment_time" property="enrollmentTime" />
            <result column="graduate_time" property="graduateTime" />
            <result column="school" property="school" />
            <result column="major" property="major" />
            <result column="education_background" property="educationBackground" />
            <result column="degree" property="degree" />
            <result column="learn_style" property="learnStyle" />
            <result column="user_id" property="userId" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
	    id AS id,
	    enrollment_time AS enrollmentTime,
	    graduate_time AS graduateTime,
	    school AS school,
	    major AS major,
	    education_background AS educationBackground,
	    degree AS degree,
	    learn_style AS learnStyle,
	    user_id AS userId
    </sql>

</mapper>
